{
  "name": "voyeur-music",
  "version": "1.0.0",
  "description": "voyeurmusic.tv",
  "main": "index.js",
  "scripts": {
    "start": "node sc2json.js && metalsmith",
    "get": "node sc2json.js",
    "build": "npm run get && metalsmith && mv build/albums/index.html build/index.html && npm run build:css",
    "build:css": "postcss --use autoprefixer -b 'last 2 versions' < build/style.css"
  },
  "author": "",
  "dependencies": {
    "autoprefixer": "^6.7.7",
    "cssmin": "^0.4.3",
    "handlebars": "^4.0.5",
    "metalsmith": "^2.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-json-to-files": "0.0.4",
    "metalsmith-layouts": "^1.4.2",
    "metalsmith-paths": "^2.1.1",
    "metalsmith-permalinks": "^0.4.1",
    "metalsmith-register-helpers": "^0.2.0",
    "postcss": "^5.2.16",
    "soundcloud-node-api": "0.0.9"
  }
}
